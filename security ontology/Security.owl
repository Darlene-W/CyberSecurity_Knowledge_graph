    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Rootkit -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Rootkit">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#MaliciousCode"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ifSuccessfulLeadsToThreat"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#MaliciousCode"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A collection of software tools installed by an intruder on a compromised computer. A rootkit will normally contain tools to hide evidence of the intrusion and backdoors to allow access or remote-control in future. Rootkits often include tools to facilitate other types of misuse such as distributed denial of service attacks or distribution of spam. (ODoC)</dc:description>
    </owl:Class>
    


