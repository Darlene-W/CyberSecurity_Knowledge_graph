    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Usurpation -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Usurpation">
