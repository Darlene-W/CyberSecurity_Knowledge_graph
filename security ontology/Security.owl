<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#"
     xml:base="http://www.ida.liu.se/~iislab/projects/secont/Security.owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:literature="http://www.ida.liu.se/~iislab/projects/secont/SecurityLiterature.owl#">
    <owl:Ontology rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl">
        <owl:imports rdf:resource="http://purl.org/dc/elements/1.1/"/>
        <owl:imports rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/SecurityLiterature.owl"/>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Almut Herzog, almhe@ida.liu.se</dc:creator>
        <dc:publisher rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dept. of Computer and Information Science, Link√∂pings universitet, Sweden</dc:publisher>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An ontology that describes security technologies as a taxonomy and as countermeasures to protection goals. Typical starting points for browsing should be the classes Asset, Countermeasure, Threat.</rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">$Id: AHz-06.7-2.5.owl,v 1.6 2006/05/30 10:51:32 almhe Exp almhe $</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/publisher -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/publisher"/>
    


    <!-- http://purl.org/dc/elements/1.1/source -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/source"/>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/SecurityLiterature.owl#hasDefinition -->

    <owl:AnnotationProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/SecurityLiterature.owl#hasDefinition"/>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/SecurityLiterature.owl#hasQuotation -->

    <owl:AnnotationProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/SecurityLiterature.owl#hasQuotation"/>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/SecurityLiterature.owl#hasSource -->

    <owl:AnnotationProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/SecurityLiterature.owl#hasSource"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#assetHasVulnerability -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#assetHasVulnerability">
        <owl:inverseOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#existsOnAsset"/>
        <rdfs:domain rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Assert"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Vulnerability"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An asset can have one or more vulnerabilities.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#containsControl -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#containsControl">
        <owl:inverseOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#isContainedIn"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A product can contain one or more countermeasure. For example: MS Word contains encryption and access control.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#containsTechnology -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#containsTechnology">
        <owl:inverseOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#residesOn"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Technology"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Technology"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">One technolgoy asset can contain further technology assets. For example: A network contains hosts, hosts contain data.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#credentialVerifiedBy -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#credentialVerifiedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#usedBy"/>
        <owl:inverseOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#verifiesCredential"/>
        <rdfs:domain rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Credential"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A credential is verified by at least one, possibly more countermeasures.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#employedAt -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#employedAt">
        <rdfs:domain rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Assert"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A countermeasure is employed at one or more points in time, e.g. at runtime, at compile time.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#enabledByVulnerability -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#enabledByVulnerability">
        <owl:inverseOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#enablesThreat"/>
        <rdfs:domain rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Attack"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Vulnerability"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A threat is enabled by one or more vulnerabilities.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#enablesThreat -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#enablesThreat">
        <rdfs:domain rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Vulnerability"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Attack"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A vulnerability enables one or more threats.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#existsOnAsset -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#existsOnAsset">
        <rdfs:domain rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Vulnerability"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Assert"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A vulnerability exists on one or more assets.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#goalHasRelatedGoal -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#goalHasRelatedGoal">
        <owl:inverseOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#goalHasRelatedGoal"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A security goal can have related security goals. For example: confidentiality is related to anonymity, privacy and secrecy.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#hasAsset -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#hasAsset">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Assert"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#hasCountermeasure -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#hasCountermeasure">
        <owl:inverseOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#protects"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Attack"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A helper property for the n-ary relations. Denotes a countermeasure.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#hasGoal -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#hasGoal">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A helper property for the n-ary relations. Denotes a goal.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#hasMechanismType -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#hasMechanismType">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A helper property for the n-ary relations. Denotes a mechanism type.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#hasThreat -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#hasThreat">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Attack"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A helper property for the n-ary relations. Denotes a threat.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#hasUser -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#hasUser">
        <rdfs:domain rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Assert"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property for determining what kind of users typically interact with a countermeasure.  A useful add-on if one wants to reason about usability requirements of countermeasures.</rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Not part of the core security ontology.</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ifSuccessfulLeadsToThreat -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ifSuccessfulLeadsToThreat">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Attack"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Threats can imply other threats. If one threat has materialised, this may open up for other threats. For example: if the host is compromised a rootkit can be introduced.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#implementsModel -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#implementsModel">
        <owl:inverseOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#modelImplementedBy"/>
        <rdfs:domain rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A countermeasure implements a theoretical security model, e.g. RBAC.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#isContainedIn -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#isContainedIn">
        <rdfs:domain rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A countermeasure is contained in one or more products. For example: Encryption part of MS Word.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#modelImplementedBy -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#modelImplementedBy">
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A model is implemented by one or more countermeasures.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#protects -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#protects">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links a countermeasure to asset, security goal and mechanism type by using the pattern of n-ary relationship as class. This class is represented by CountermeasureProtectsGoalOfAssetThroughMechanismtype.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#residesOn -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#residesOn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Technology"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">One technology asset resides on one or more technology assets. For example: Data resides on Host, Host resides on Network.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#runsOnOperatingSystem -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#runsOnOperatingSystem">
        <owl:inverseOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#supportsCountermeasure"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#OperatingSystem"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A countermeasure executes on one or more operating systems. This is for finding countermeasure products for a given operating system. For example: Which firewalls run on Mac OS?</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#supportedBy -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#supportedBy">
        <owl:inverseOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#supports"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A countermeasure is supported (relates to) another countermeasure. Encryption is supported by key management.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#supports -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#supports">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#supportsCountermeasure -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#supportsCountermeasure">
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An operating system can support one or more countermeasures. This is for finding countermeasure products for a given operating system. For example: Which firewalls run on Mac OS?</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#threatens -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#threatens">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Attack"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links a threat to asset and security goalby using the pattern of n-ary relationship as class. This class is represented by ThreatThreatensGoalOfAsset.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#usedBy -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#usedBy">
        <owl:inverseOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#uses"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A countermeasure or credential is used by one or more countermeasures or credentials.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#uses -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#uses">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A countermeasure or a credential can use one or more countermeasures or credentials.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#verifiesCredential -->

    <owl:ObjectProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#verifiesCredential">
        <rdfs:subPropertyOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#uses"/>
        <rdfs:domain rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <rdfs:range rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Credential"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A countermeasure may verify one or more credentials. For example: DigitalSignatureValidation verifies DigitalSignature.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#hasStandardReference -->

    <owl:DatatypeProperty rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#hasStandardReference">
        <rdfs:domain rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Standard"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Supplies a reference to the standard.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#AES -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#AES">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BlockCipher"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Standard"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SymmetricAlgorithm"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A US government encryption standard that supercedes DES. AES specifies the Rijndael algorithm with key sizes of 128, 192, and 256 bits and a block size of 128 bits. (Phoha)</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Advanced Encryption Standard.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#AccessControl -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#AccessControl">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The prevention of unauthorized use of a resource, including the prevention of use of a resource in an unauthorised manner. (CCITT)</dc:description>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The process of ensuring that only authorized subjects are allowed access to a resource. (generalised from ODoC)</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This is the enforcement part. The authentication part of access control is handled in AuthenticationService/Loginsystem.
Access control does prevent bad things from happening.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ActiveAttack -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ActiveAttack">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Attack"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Classification from ODotI, together with PassiveAttack
NP6: Active misuse of resources. Misuse of (apparently) conferred authority that alters the system or its data.
integrity of the asset is violated (my def)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ActiveHardwareMisuse -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ActiveHardwareMisuse">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Disruption"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NP2b) active hardware misuse with side effects</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ActiveIntrusionDetectionSystem -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ActiveIntrusionDetectionSystem">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ByBehaviourOnDetection"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The IDS responds to the suspicious activity by e.g. logging off a user or by reprogramming the firewall to block network traffic from the suspected malicious source. (Webopedia)</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Aka reactive IDS.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#AdHocNetwork -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#AdHocNetwork">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Network"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#AnomalyDetectionSystem -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#AnomalyDetectionSystem">
        <owl:equivalentClass rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Behaviour-BasedIntrusionDetectionSystem"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ByDetectionMethod"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">In anomaly detection, the system administrator defines the baseline, or normal, state of the network?s traffic load, breakdown, protocol, and typical packet size. The anomaly detector monitors network segments to compare their state to the normal baseline and look for anomalies. (Webopedia)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Anti-VirusSoftware -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Anti-VirusSoftware">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#AccessControl"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Monitoring"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Software which monitors a computer and its files in order to detect any virus intrusions that have occurred (or is occuring(my def)) (ODotI)</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">At best AV software provides prevention, detection and recovery. At least, it provides detection.
</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Application -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Application">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Software"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ApplicationLayerPacket -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ApplicationLayerPacket">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DataInTransit"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ApplicationLevelGateway -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ApplicationLevelGateway">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Firewall"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Aka proxy server, relay of application-level traffic. If  the gateway does not implement the proxy code for a specific application, the service is not supported and cannot be forwarded. Scrutinizes only a few allowable applications. (Stallings06)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Assert -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Assert">
        <owl:disjointWith rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Vulnerability"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A useful or valuable thing. (OED)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Attack -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Attack">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#enabledByVulnerability"/>
                <owl:allValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Vulnerability"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Vulnerability"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A potential violation of security. (CCITT)
Any event that is potentially harmful to an IS through unauthorised access, destruction, disclosure, modification of data, and/or denial of service. (ISD)</dc:description>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A synonym for threat. Some publications use threat for high-level threats like &quot;act of human error or failure&quot; and attack for more specific threats like &quot;virus&quot;. But the distinction is arbitrary as illustrated in Whitford, Mattord, Principles of Information Security, 2nd Edition, 2005. p. 126: Table 4.3 describes threats to information security, one threat being &apos;deliberate software *attacks*&apos;.</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Auditing -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Auditing">
        <owl:equivalentClass rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#LogMonitoring"/>
        <owl:equivalentClass rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Logging"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Monitoring"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Collection of data about events which have occurred in a system. (inspired by ODotI)</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Same as: Logging, LogMonitoring</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Backdoor -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Backdoor">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#MaliciousCode"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A backdoor is the malicious hacker&apos;s tool of choice that allows remote connections to systems. A typical backdoor opens a network port (UDP/TCP) on the host when it is executed. Then the listening backdoor waits for a remote connection from the attacker and allows the attacker to connect to the system. (Szor)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Backup -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Backup">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Copies of files, data and programs made to facilitate recovery from failures of primary system. (Phoha)</dc:description>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The transfer of file which are stored on a computer to some long-term archival medium such as a CD ROM. (ODotI)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BackupFile -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BackupFile">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#File"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Bacteria -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Bacteria">
        <owl:equivalentClass rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Rabbit"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Virus"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Same as Rabbit (Bishop)</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BastionHost -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BastionHost">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#NetworkedHost"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A computer which stands between a secure network and the Internet. (ODotI)
A firewall host which acts as an interface point to an external untrusted network. (Phoha)</dc:description>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bastion Host</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Behaviour-BasedIntrusionDetectionSystem -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Behaviour-BasedIntrusionDetectionSystem">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ByDetectionMethod"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BehaviouralBiometricCredential -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BehaviouralBiometricCredential">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BiometricToken"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#credentialVerifiedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BehaviouralBiometricRecognition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BehaviouralBiometricRecognition -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BehaviouralBiometricRecognition">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BiometricAuthenticationSystem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#verifiesCredential"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BehaviouralBiometricCredential"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#verifiesCredential"/>
                <owl:allValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BehaviouralBiometricCredential"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BiometricAuthenticationSystem -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BiometricAuthenticationSystem">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#LoginSystem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#verifiesCredential"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BiometricToken"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#verifiesCredential"/>
                <owl:allValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BiometricToken"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A system that authenticates people using biometrics, i.e. that rely on the measurement of an anatomical, physiological or behavioural characteristic of the user, rather than knowledge or possession of information or a key. (Phoha)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BiometricToken -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BiometricToken">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Credential"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#credentialVerifiedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BiometricAuthenticationSystem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BlockCipher -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BlockCipher">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#EncryptionAlgorithm"/>
        <owl:disjointWith rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#EncryptionHardware"/>
        <owl:disjointWith rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#StreamCipher"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A cipher in which a fixed-length block of data is encrypted, or decrypted, at each iteration of the algorithm: each block is input, encrypted, and output, with no memory (to retain message-dependent information) between blocks. (ODoc)</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Block ciphers do typically also protect the integrity of data. It is hard to modify the cryptotext so that the recipient does not detect the change.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Blowfish -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Blowfish">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BlockCipher"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Standard"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SymmetricAlgorithm"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BruteForceAttack -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BruteForceAttack">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ActiveAttack"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Any criminal attempt to access a computer system by the repeated execution of some action. (ODotI)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BufferOverflow -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BufferOverflow">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#MalformedInput"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Buffer overflow happens when input or intermediate results exceed the buffer size. (Phoha)
A program writes data beyond the allocated end of a buffer in memory. Buffer overflows usually arise as a consequence of a bug and the use of languages such as C or C++ that are not &quot;memory-safe&quot;. One consequence of the overflow is that valid data can be overwritten as a result. (Wikipedia)
A method of overloading a predefined amount of space in a buffer, which can potentially overwrite and corrupt data in memory.
(www.evestigations.com/html/glossary.html)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ByAuditSourceLocation -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ByAuditSourceLocation">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#IntrusionDetectionSystem"/>
        <rdfs:isDefinedBy rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/SecurityLiterature.owl#DDW99"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ByBehaviourOnDetection -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ByBehaviourOnDetection">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#IntrusionDetectionSystem"/>
        <rdfs:isDefinedBy rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/SecurityLiterature.owl#DDW99"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ByDetectionMethod -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ByDetectionMethod">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#IntrusionDetectionSystem"/>
        <rdfs:isDefinedBy rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/SecurityLiterature.owl#DDW99"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ByUsageFrequency -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ByUsageFrequency">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#IntrusionDetectionSystem"/>
        <rdfs:isDefinedBy rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/SecurityLiterature.owl#DDW99"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BypassingIntendedControls -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BypassingIntendedControls">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ActiveAttack"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NP5: Circumvention of existing controls or improper acquisition of otherwise denied authority.</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CBC-MAC -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CBC-MAC">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#MACAlgorithm"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The simplest way to make a key-dependent one-way hash function is to encrypt a message with a block algorithm in CBC or CFB modes . The hash is the last encrypted block, encrypted once more in CBC or CFB modes.</dc:description>
        <rdfs:isDefinedBy rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Sch96"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CPU -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CPU">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Hardware"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CachePoisoning -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CachePoisoning">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DNSHacking"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Masquerading"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The DNS server is made to cache bogus information, the attacker can redirect traffic intended for a legitimate site to a site under the attacker&apos;s control. (CM02)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CertificateData -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CertificateData">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DigitalSignatureData"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#credentialVerifiedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SigningWithCertificate"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A certificate authority (CA) issues an encrypted digital certificate containing the applicant&apos;s public key and a variety of other identification information. The CA makes its own public key readily available through print publicity or perhaps on the Internet. The recipient of an encrypted message uses the CA&apos;s public key to decode the digital certificate attached to the message, verifies it as issued by the CA and then obtains the sender&apos;s public key and identification information held within the certificate. With this information, the recipient can send an encrypted reply. (Webopedia)</dc:description>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A data structure signed with a public key digital signature stating that a specified public key belongs to someone or something with a specified identification. (ISD)</dc:description>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A document with a digital signature. Such a certificate is used to authenticate identity. Digital certificates are often signed by a trusted third party known as a certificate authority.  (ODotI)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Checksum -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Checksum">
        <owl:equivalentClass rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#MessageDigest"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ChecksumAlgorithm -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ChecksumAlgorithm">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Encryption"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Algorithms for creating checksums. These checksums are for error detection and possibly correction on a low level, e.g. disturbances over a noisy connection. They are not secure to use for high-level integrity as new checksums can easily be created and small changes in the data can result in the same checksum. For stronger checksum algorithms, see the class cryptographic hash function.</dc:description>
        <rdfs:isDefinedBy rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#KLK05"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ChecksumData -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ChecksumData">
        <owl:equivalentClass rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#MessageDigestData"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ElectronicToken"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A small fixed-length quantity computed as a function of an arbitrary-length message. A checksum is computed by the sender of a message, recomputed and checked by the recipient of a message to detect data corruption. ... Usage has extended the definition to include more complex noncryptographic functions such as CRCs... and cryptographic functions such as message digests. (Phoha)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CircuitLevelGateway -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CircuitLevelGateway">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Firewall"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A circuit-level gateway does not permit an end-to-end TCP connection, but sets up two TCP connections, one between itself and a TCP user on an inner host and one between itself and a TCP user on an outside host. The gateway typically relays TCP segments from one connection to the other without examining the contents. The security function consists of determining which connections will be allowed. (Stallings06)</dc:description>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ensures the validity of TCP and UDP sessions by ceating a handshake between communicating parties and passing packets through until the end of the session. (ISD)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ClientHost -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ClientHost">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#NetworkedHost"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">As opposed to ServerHost. Important in authentication.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Code -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Code">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#VulObject"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CodeInjection -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CodeInjection">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#MalformedInput"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ifSuccessfulLeadsToThreat"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#MaliciousCode"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">technique to introduce arbitrary code into a running computer process (Wikipedia). Subcategories also from Wikipedia.</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CodeSigning -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CodeSigning">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DigitalSigning"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A secure hashing algorithm is used on the code to be executed providing a compact code signature. This signature is encrypted using the private key of a vendor. ... (Phoha)</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CodeSigning may overlap with SigningWithCertificate. CodeSigning is specific to Code, not to Signing.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Configuration -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Configuration">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#VulObject"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ConfigurationFile -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ConfigurationFile">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#File"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A file that configures a program, also a policy.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ContinuousIntrusionDetectionSystem -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ContinuousIntrusionDetectionSystem">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ByUsageFrequency"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Cookie -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Cookie">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ElectronicToken"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <owl:disjointWith rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Vulnerability"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A countermeasure is a piece of &apos;security technology&apos; that protects assets against threats. An alternative naming is &apos;control&apos;. (my def)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Credential -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Credential">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Assert"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#credentialVerifiedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#credentialVerifiedBy"/>
                <owl:allValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A credential is a piece of data or hardware that is presented to an AuthenticationService or IntegrityValidationService. 



</dc:description>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An object that is verified when presented to the verifier in an authentication transaction. Credentials may be bound in some way to the individual to whom they were issued, or they may be bearer credentials. The former are necessary for identification, while the latter may be acceptable for some forms of authorization. (Webopedia)</dc:description>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Data that is transferred to establish the claimed identity of an entity. (CCITT)</dc:description>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Secret information used to prove one&apos;s identity or authorisation in an authentication exchange. (ISD)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CrossSiteScripting -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CrossSiteScripting">
        <owl:equivalentClass rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#HTMLScriptInjection"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CodeInjection"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CryptographicHashFunction -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CryptographicHashFunction">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SignatureAlgorithm"/>
        <owl:disjointWith rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#EncryptionHardware"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A cryptographic hash function is a hash function with certain additional security properties (impossible to find a (previously unseen) message that matches a given digest, impossible to find &quot;collisions&quot;, wherein two different messages have the same message digest). A hash function takes a long string (or message) of any length as input and produces a fixed length string as output, sometimes termed a message digest or a digital fingerprint. (summarised from Wikipedia)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Cryptography -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Cryptography">
        <owl:equivalentClass rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Encryption"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Discipline which embodies principles, means, and methods for the transformation of data in order to hide its information content, prevent its undetected modification and/or prevent its unauthorised use. (CCITT)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DBMS -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DBMS">
        <owl:equivalentClass rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Database"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Software"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Database Management System</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DES -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DES">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BlockCipher"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Standard"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SymmetricAlgorithm"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A US government standard for a block cipher which employs a 56-bit secret key. (ODoC)</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Data Encryption Standard, predecessor of AES.
</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DNSHacking -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DNSHacking">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#InternetInfrastructureAttack"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Any attack that affects the DNS services. (CM02)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DNSSEC -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DNSSEC">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SecureNetworkCommunication"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Standard"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#uses"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DigitalSignatureData"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This protocol provides security extensions to the DNS to assure data integrity or authentication. DNSSEC provides data integrity and authentication services to security-aware resolvers or applications through the use of cryptographic digital signatures. (ISD)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DNSServerCompromising -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DNSServerCompromising">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DNSHacking"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#HostCompromising"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The DNS server is compromised and thus under full control of the attacker. DNS data is no longer trusted. (CM02)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DNSSpoofing -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DNSSpoofing">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DNSHacking"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Masquerading"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The attacker masquerades as a DNS server and feeds the client wrong and/or potentially malicious information. (CM02)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DSA -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DSA">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CryptographicHashFunction"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Standard"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#hasStandardReference"/>
                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string">FIPS 186-2</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Digital Signature Algorithm, an algorithm which is used in the US government&apos;s digital signature standard. When applied to a message it generates two integers which need to be compared by the recipient of a message which has been signed using this technique. The algorithm can only be used for digital signatures, not for the transmission of data. (ODotI)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Damage -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Damage">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PhysicalThreat"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Data -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Data">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Technology"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DataInTransit -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DataInTransit">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Data"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#residesOn"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Network"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Data that is transferred over a network. Notice that all other subclasses of Data can also be &quot;in transit&quot;. A login name, password, or a copy of the heap may be transferred over the network.</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DataOnNon-VolatileMedia -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DataOnNon-VolatileMedia">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#StationaryData"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DataOnVolatileMedia -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DataOnVolatileMedia">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#StationaryData"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Database -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Database">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Software"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DatabaseDataFile -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DatabaseDataFile">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#File"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The data files, as distinguished from the executables, of a database management system.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Deception -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Deception">
        <owl:equivalentClass rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Impersonation"/>
        <owl:equivalentClass rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Masquerading"/>
        <owl:equivalentClass rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Spoofing"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ActiveAttack"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Acceptance of false data (Shi94)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Degausser -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Degausser">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Sanitizer"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SecurityHardware"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A device that can generate a magnetic field for degaussing magnetic storage media. (www.all.net/books/standards/remnants/glo.html)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DenialOfService -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DenialOfService">
