        <owl:disjointWith rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#UntrustedNetwork"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Not so sure if this is needed. Could not find a use case yet.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#UDPPacket -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#UDPPacket">
