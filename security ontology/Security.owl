        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#verifiesCredential"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CertificateData"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


