    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ShellInjection -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ShellInjection">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CodeInjection"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SideChannelAttack -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SideChannelAttack">
