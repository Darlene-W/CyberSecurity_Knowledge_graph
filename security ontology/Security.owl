        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unauthorised control of some part of the system (Shi94)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#VPN -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#VPN">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SecureNetworkCommunication"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#uses"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SSH"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Virtual private network. A connection between two computers or other endpoints in which packets sent over a shared network are automatically encrypted and decrypted by the endpoints. (ODoC)</dc:description>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">VPN (Virtual Private Network)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Virus -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Virus">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#MaliciousCode"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Code that recursively replicates a possibly evolved copy of itself. Viruses infect a host file or system area, or they simply modify a reference to such objects to take control and then multiply again to form new generations. (Szor)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Voice -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Voice">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BehaviouralBiometricCredential"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#credentialVerifiedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#VoiceRecognition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#VoiceRecognition -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#VoiceRecognition">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BehaviouralBiometricRecognition"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#verifiesCredential"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Voice"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#verifiesCredential"/>
                <owl:allValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Voice"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#VulObject -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#VulObject">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Vulnerability"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#VulProperty -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#VulProperty">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Vulnerability"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Vulnerability -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Vulnerability">
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Any mechanism that could lead to a breach of the security of a system in the presence of a threat. (ODoC)
Asset: Building, Threat: Flood, Vulnerability: Closeness to water, meters above water level.</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#VulnerabilityScanner -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#VulnerabilityScanner">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SystemMapping"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A vulnerability scanner is a type of computer program specifically designed to search a given target (piece of software, computer, network, etc.) for weaknesses. The scanner systematically engages the target in an attempt to assess where the target is vulnerable to &quot;attack&quot;. The program can be used either prophylactically (to find holes and plug them before they are exploited) or maliciously (to find holes and exploit them). (Wikipedia)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#WindowAttack -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#WindowAttack">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DenialOfService"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A form of DENIAL OF SERVICE ATTACK which relies on the fact that administering visible widgets such as WINDOWS and BUTTONS on a screen consumes a large amount of resources. A typical example of a window attack might involve the creation of a large number of windows displayed on a screen. The amount of resources required to manage such a large number of windows can easily render a computer inoperable. (ODotI)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Windows -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Windows">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#OperatingSystem"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#WiredNetwork -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#WiredNetwork">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Network"/>
        <owl:disjointWith rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#WirelessNetwork"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#WirelessAccessPoint -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#WirelessAccessPoint">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Router"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#WirelessNetwork -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#WirelessNetwork">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Network"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Worm -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Worm">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Virus"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Worms are network viruses, primarily replicating on networks. (Szor)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#X.509 -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#X.509">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PKI"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SecureNetworkCommunication"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Standard"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The most popular standard for security certificates. It contains information such as the public key associated with some Internet entity, its name, and a unique registration number. (ODotI)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#X.509CertificateData -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#X.509CertificateData">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CertificateData"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#credentialVerifiedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SigningWithX.509Certificate"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#uses"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#X.509"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A certificate based on the X.509 standard. (my def)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#_2TDES -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#_2TDES">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TripleDES"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Like TDES-EDE but with only two keys: DES(k1;DES^-1(k2;DES(k1;M)))</dc:description>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2TDES</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#_3TDES -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#_3TDES">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TripleDES"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Uses three different keys: 
DES(k3;DES(k2;DES(k1;M)))
where M is the message block to be encrypted and k1, k2, and k3 are DES keys</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Not allowed to start name with number, thus the underscore in the name.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">3TDES</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#user -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#user">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Application"/>
    </owl:Class>
    


