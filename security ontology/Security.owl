        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Acquiring data from object residue. (ISD)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SecureNetworkCommunication -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SecureNetworkCommunication">
