        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SymmetricAlgorithm"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A secret key encryption algorithm developed by NSA (US) using 64-bit blocks and 80-bit keys. (ISD)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SmartCard -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SmartCard">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PhysicalToken"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SecurityHardware"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> A small electronic device about the size of a credit card that contains electronic memory, and possibly an embedded integrated circuit (IC). (Webopedia) A smartcard can be used as an access control token that proves the autorisation of its bearer. (my def)</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">It is either used as human authentication or for data confidentiality, possibly both.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Smurf -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Smurf">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DenialOfService"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Masquerading"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A DoS-attack in which many pings (ICMP echo request packets) are broadcast to the network. The source field is set to the victim&apos;s IP address. Any machines that respond will transmit to the victim, overloading its network interface. (ISD)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Snort -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Snort">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Network-BasedIntrusionDetectionSystem"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Network-based intrusion detection system. GP license.</dc:description>
        <dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.snort.org/</dc:source>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SocialAttack -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SocialAttack">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#HumanAttack"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SocialEngineer -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SocialEngineer">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ExploitMethod"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Software -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Software">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Technology"/>
        <owl:disjointWith rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Vulnerability"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A product that contains countermeasures.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SourceCodeAnalysis -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SourceCodeAnalysis">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#employedAt"/>
                <owl:hasValue rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#AtWriteTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Aka static code analysis Tools that statically analyse source or object code for problems, flag suspicious constructs in software (adapted from Wikipedia)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SourceRoutingAttack -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SourceRoutingAttack">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BypassingIntendedControls"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The source station specifies the route that a packet should take as it crosses the Internet, in the hopes that this will bypass security measures that do not analyse the source routing information. The countermeasure is to discard all packets that use this option. (Stallings 06)</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This attack may accomplish all sorts of things, thus we do not (on this level) define any existential quantifiers.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Spoofing -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Spoofing">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ActiveAttack"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Spyware -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Spyware">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#MaliciousCode"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Any software that covertly gathers user information through the user&apos;s Internet connection without his or her knowledge, usually for advertising purposes. (Webopedia)</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Spyware threatens the integrity (unauthorised software) and availability (software consumes resources) of the host on which it is executed. It also threatens the user&apos;s privacy.</rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TODO: Spyware could also be used as a countermeasure to guard policy compliance, if installed by an authorised entity.</owl:versionInfo>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Stack -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Stack">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DataOnVolatileMedia"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#StackAttack -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#StackAttack">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DenialOfService"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A stack attack involves running a program, typically from a web page, which consumes huge amounts of stack memory. (ODotI)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#StackOverflow -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#StackOverflow">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BufferOverflow"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ifSuccessfulLeadsToThreat"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#MaliciousCode"/>
                            <rdf:Description rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Usurpation"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Standard -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Standard">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#StatefulInspectionPacketFilter -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#StatefulInspectionPacketFilter">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PacketFilter"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Considers state of higher-level connection, not only single packets of stateful protocols. (Stallings 06)</dc:description>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Stateful Inspection Packet Filter</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#StationaryData -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#StationaryData">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Data"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#residesOn"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Host"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#residesOn"/>
                <owl:allValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Host"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#StatisticalAttack -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#StatisticalAttack">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PassiveAttack"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An attack on a database to which users are allowed limited access: for example, access in order to calculate information such as averages. A statistical attack uses this information to discover data that is secret.  (ODotI)
Is this really a passive attack? Even if it leaves data unmodified? (my)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Steganography -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Steganography">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Encryption"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Means by which two or more parties may employ subliminal or invisible communication. (ISD)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#StreamCipher -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#StreamCipher">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#EncryptionAlgorithm"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A cipher in which a relatively small quantity of data is encrypted (or decrypted) at each iteration of the algorithm, perhaps just one character or byte, but in which the algorithm contains memory that retains message-dependent information between iterations. (ODoc)</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Not all stream ciphers are guaranteed to provide the detection of integrity violations. This must be supplied for every algorithm.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SwitchingOffFan -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SwitchingOffFan">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Heat"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SymmetricAlgorithm -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SymmetricAlgorithm">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#EncryptionAlgorithm"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#uses"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SymmetricKey"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">[...] The same key is used to encrypt and decrypt the text that is to be sent. This means that the sender and the receiver must have exchanged this key some time before the message has been sent and that this transfer must occur over a secure channel. The key used for symmetric key encryption is often known as a SECRET KEY. (ODoC)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SymmetricKey -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SymmetricKey">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#EncryptionKey"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The same key is used in encryption and decryption.</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SystemMapping -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SystemMapping">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PassiveAttack"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tools used for learning more about a system, host, network etc.</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">System Mapping</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SystemModification -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SystemModification">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ActiveAttack"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ifSuccessfulLeadsToThreat"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#MaliciousCode"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NP 4:Setting Up Subsequent Misuse: Planting and arming malicious software
Introduction of unauthorised or unwanted software.</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TCPPacket -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TCPPacket">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TransportLayerPacket"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TDES-EDE -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TDES-EDE">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TripleDES"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TripleDES used in Encryption-Decryption-Encryption mode for compatibility with single DES: 
DES(k3;DES^‚àí1(k2;DES(k1;M))) and so a single DES encryption with key k can be represented as TDES-EDE with k1 = k2 = k3 = k.</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Technology -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Technology">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Assert"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TechnologyMistakenlyUsedAsCountermeasure -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TechnologyMistakenlyUsedAsCountermeasure">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sometimes a technology is used as a countermeasure but it was not devised to be a countermeasure and may possibly not the best suitable countermeasure against a perceived threat. Such technologies are put into this class as a caveat.</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Theft -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Theft">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PhysicalThreat"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TimingAttack -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TimingAttack">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SideChannelAttack"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A timing attack is a form of side channel attack where the attacker tries to break a cryptosystem by analyzing the time taken to execute cryptographic algorithms. The attack exploits the fact that, particularly in asymmetric key algorithms, computation time for a private key operation is dependent on the key in some way. (Wikipedia) (Term also badly explained in ODotI)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TinyFragmentAttack -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TinyFragmentAttack">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BypassingIntendedControls"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The attacker uses the IP fragmentation option to create extremely small fragments and force the TCP header information into a separate packet fragment. This attack is designed to circumvent filtering rules that depend on TCP header information. (Stallings 06)</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This attack may accomplish all sorts of things, thus we do not (on this level) define any existential quantifiers.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TrafficAnalysis -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TrafficAnalysis">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Eavesdropping"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">That part of eavesdropping concerned with the analysis of which parties are communicating and the volumes and timings of those communications, rather than the contents of the messages themselves. Some encryption systems do not protect users against traffic analysis, even though all the content my be encrypted. (ISD)
Traffic analysis is the process of intercepting and examining messages in order to deduce information from patterns in communication. It can be performed even when the messages are encrypted and cannot be decrypted. In general, the greater the number of messages observed, or even intercepted and stored, the more that can be inferred from the traffic. (Wikipedia)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TrafficPadding -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TrafficPadding">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SecureNetworkCommunication"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The addition of false communications or data units to conceal the amount of real data units being sent. (ISD)
The generation of spurious instances of communication, spurious data units and/or spurious data within data units. (CCITT)</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TODO: Find a name for the asset that is protected by traffic padding, add that asset to the asset hierarchy and introduce it as protection goal with the protects-property.</owl:versionInfo>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TransportLayerPacket -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TransportLayerPacket">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DataInTransit"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TripleDES -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TripleDES">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BlockCipher"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SymmetricAlgorithm"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#uses"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DES"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Uses three rounds of DES. Different variants exist, see subclasses.</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Tripwire -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Tripwire">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Host-BasedIntrusionDetectionSystem"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Host-based intrusion detection system. Free and open source.</dc:description>
        <dc:source rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://sourceforge.net/projects/tripwire</dc:source>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TrojanHorse -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TrojanHorse">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#MaliciousCode"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An apparently innocent program designed to circumvent the security features of a system.  (ODoC)
A virus which infects a computer by masquerading as a normal system entity.  (ODotI) (too strict, a modified ps-command is a Trojan but not a virus)
</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TrustManagement -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TrustManagement">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a uniÔ¨Åed approach to specifying and interpreting security policies,
credentials, and relationships which allow direct authorization of
security-critical actions (M. Blaze, 1996)</dc:description>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the activity of collecting, encoding, analyzing and presenting evidence
relating to competence, honesty, security or dependability with the
purpose of making assessments and decisions regarding trust relationships
(Grandison, 2000)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TrustedNetwork -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#TrustedNetwork">
