            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#verifiesCredential"/>
                <owl:allValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#X.509CertificateData"/>
