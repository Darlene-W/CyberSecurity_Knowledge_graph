    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DigitalSignatureData -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DigitalSignatureData">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#MessageDigestData"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#uses"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Public-KeyEncryption"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#uses"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CryptographicHashFunction"/>
                            <rdf:Description rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Public-KeyEncryption"/>
                            <rdf:Description rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#X.509"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A digital code that can be attached to an electronically transmitted message that uniquely identifies the sender. (Webopedia)</dc:description>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An authentication code appended to an electronic document that indicates the author of the document. (ODoC)</dc:description>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Data appended to, or a cryptographic transformation of a data unit that allows a recipient of the data unit to prove the source and integrity of the data unit and protect against forgery by the recipient. (CCITT)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DigitalSigning -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DigitalSigning">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#MessageDigest"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#uses"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CryptographicHashFunction"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#verifiesCredential"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#DigitalSignatureData"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#verifiesCredential"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#MessageDigestData"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The process of (1) signing digital data, typically before deployment, typically performed by the data owner or producer and (2) verifying the digital signature at a later stage, typically after deployment, typically performed by the data consumer.</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This class denotes the first meaning of the definition, namely the signing part. The verification part is called DigitalSignatureValidation in this ontology.</rdfs:comment>
    </owl:Class>
    


