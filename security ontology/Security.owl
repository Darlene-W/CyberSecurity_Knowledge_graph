        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Randomly contact hosts to find interesting hosts, may e.g. use war dialling (adapted from ODotI)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Scavenging -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Scavenging">
