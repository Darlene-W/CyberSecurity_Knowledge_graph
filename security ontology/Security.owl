    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PacketMistreatmentRouter -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PacketMistreatmentRouter">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PacketMistreatment"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PassiveAttack -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PassiveAttack">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Attack"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Classification from ODotI, together with ActiveAttack
Also fits NP7: Passive misuse of resources
Confidentiality of the asset is violated but nothing is modified.</dc:description>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PassiveIntrusionDetectionSystem -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PassiveIntrusionDetectionSystem">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ByBehaviourOnDetection"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The IDS detects a potential security breach, logs the information and signals an alert. (Webopedia)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Password -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Password">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ElectronicToken"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Confidential authentication information, usually composed of a string of characters. (CCITT)
A unique character string held by each user, a copy of which is stored within the system. During login an authentication process takes place; the password entered by the intending user must correspond with the stored value before the user is accepted by the system. (ODoC)
A secret series of characters that enables a user to access a file, computer, or program. On multi-user systems, each user must enter his or her password before the computer will respond to commands. The password helps ensure that unauthorized users do not access the computer. In addition, data files and programs may require a password. (Webopedia)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PasswordSniffing -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PasswordSniffing">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Eavesdropping"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Eavesdropping to capture passwords, which can then be used to masquerade as a legitimate user. (ISD)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PeriodicIntrusionDetectionSystem -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PeriodicIntrusionDetectionSystem">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ByUsageFrequency"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Phishing -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Phishing">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ExploitMethod"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Masquerading"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A form of fraud, often using forged e-mails or Web sites, that tries to persuade users to disclose usernames and passwords for on-line bank accounts or other valuable resources. (ODoc)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PhysicalAttack -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PhysicalAttack">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#HumanAttack"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PhysicalBiometricCredential -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PhysicalBiometricCredential">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BiometricToken"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#credentialVerifiedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PhysicalBiometricRecognition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PhysicalBiometricRecognition -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PhysicalBiometricRecognition">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#BiometricAuthenticationSystem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#verifiesCredential"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#PhysicalBiometricCredential"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
