        <owl:disjointWith rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ShellInjection"/>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SSH -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SSH">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SecureNetworkCommunication"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Standard"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#uses"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Public-KeyEncryption"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#uses"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SymmetricAlgorithm"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Secure Shell, a network protocol designed for logging into and executing commands on a networked computer, replacing the insecure ftp, telnet and rlogin protocol. (Wikipedia)</dc:description>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SSH (Secure Shell)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SSL -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SSL">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SecureNetworkCommunication"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Standard"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#uses"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Public-KeyEncryption"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#uses"/>
                <owl:someValuesFrom rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SymmetricAlgorithm"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Secure Sockets Layer, a protocol developed by Netscape for transmitting private documents via the Internet. SSL uses a cryptographic system that uses two keys to encrypt data ? a public key known to everyone and a private or secret key known only to the recipient of the message. (Webopedia)
It is a flexible protocol that is able to use a variety of encryption schemes. (ODotI)</dc:description>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SSL (Secure Sockets Layer)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Sanitizer -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Sanitizer">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Countermeasure"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">leads to the unrecoverable erasure of sensitive material from a system (adapted from ODoC)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ScanningAttack -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#ScanningAttack">
