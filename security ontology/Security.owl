        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Standard"/>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Security Assertion Markup Language, an XML-based framework for ensuring that transmitted communications are secure. SAML defines mechanisms to exchange authentication, authorization and nonrepudiation information, allowing single signon capabilities for Web services. (Webopedia)</dc:description>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SAML (Security Assertion Markup Language)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SHA-1 -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SHA-1">
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#CryptographicHashFunction"/>
        <rdfs:subClassOf rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#Standard"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#hasStandardReference"/>
                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string">US government standard</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Secure Hash Algorithm 1, a message digest algorithm that has been developed by the US government. (ODotI)</dc:description>
    </owl:Class>
    


    <!-- http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SQLInjection -->

    <owl:Class rdf:about="http://www.ida.liu.se/~iislab/projects/secont/Security.owl#SQLInjection">
